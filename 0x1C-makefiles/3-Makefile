CC = gcc
SRC = main.c school.c
OBJ = main.o school.o
NAME = school
CFLAGS = -o
RM = rm

all: $(OBJ)
	$(CC) $(OBJ) $(CFLAGS) $(NAME)

main.o: main.c m.h
	gcc -c -o main.o main.c

school.o: school.c m.h
	gcc -c -o school.o school.c

clean:
	$(RM) -f *~ $(NAME)

oclean: 
	$(RM) -f $(OBJ)

fclean: clean oclean 

re: oclean all
